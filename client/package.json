{
  "name": "heartsteps",
  "version": "2.0.0",
  "author": "Nick Reid",
  "homepage": "http://nickreid.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "build:app": "ionic-app-scripts build --appEntryPoint /client/src/app/main.ts",
    "build:app:android": "ionic cordova build android --release --buildConfig=build.json --appEntryPoint /client/src/app/main.ts",
    "build:app:android:debug": "ionic cordova build android --buildConfig=build.json --appEntryPoint /client/src/app/main.ts",
    "serve:app": "ionic-app-scripts serve --appEntryPoint /client/src/app/main.ts",
    "build:website": "ionic-app-scripts build --appEntryPoint /client/src/website/main.ts --appNgModulePath /client/src/website/app.module.ts",
    "serve:website": "ionic-app-scripts serve --appEntryPoint /client/src/website/main.ts --appNgModulePath /client/src/website/app.module.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/cdk": "5.2.1",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "@ionic-native/core": "4.11.0",
    "@ionic-native/http": "^4.9.1",
    "@ionic-native/in-app-browser": "^4.15.0",
    "@ionic-native/safari-view-controller": "^4.15.0",
    "@ionic-native/splash-screen": "4.7.0",
    "@ionic-native/status-bar": "4.7.0",
    "@ionic/storage": "2.1.3",
    "axios": "^0.18.0",
    "cordova-android": "^8.0.0",
    "cordova-inappbrowser": "^1.0.6",
    "cordova-ios": "^5.0.0",
    "cordova-plugin-advanced-http": "^1.11.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-inappbrowser": "^3.0.0",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-safariviewcontroller": "^1.5.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "d3": "^5.7.0",
    "firebase": "^5.1.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "moment": "^2.24.0",
    "node-sass": "^4.9.0",
    "onesignal-cordova-plugin": "^2.4.6",
    "rxjs": "5.5.11",
    "sw-toolbox": "3.6.0",
    "url-join": "^4.0.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.9",
    "@types/d3": "^5.0.1",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.10.1",
    "cordova-icon": "^1.0.0",
    "cordova-splash": "^1.0.0",
    "dotenv": "^6.0.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "tslint": "^5.11.0",
    "typescript": "~2.6.2"
  },
  "description": "An Ionic project",
  "config": {
    "ionic_copy": "./config/ionic-copy-config.js",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "<rootDir>/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@pages/(.*)": "<rootDir>/src/pages/$1",
      "@infrastructure/(.*)": "<rootDir>/src/infrastructure/$1",
      "@heartsteps/(.*)": "<rootDir>/src/heartsteps/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.ts",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|@ionic-native|@ionic)"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "comnickreidheartsteps",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "onesignal-cordova-plugin": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}